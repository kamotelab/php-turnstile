{
  "name": "kamotelab/php-turnstile",
  "type": "library",
  "description": "PHP implementation of cloudflare's turnstile",
  "keywords": [
    "php-turnstile",
    "php",
    "turnstile"
  ],
  "homepage": "https://github.com/kamotelab/php-turnstile",
  "license": "MIT",
  "authors": [
    {
      "name": "Yan Santos"
    },
    {
      "name": "Kamote Lab",
      "homepage": "https://github.com/KamoteLab"
    }
  ],
  "autoload": {
    "psr-4": {
      "KamoteLab\\": "src/"
    }
  },
  "require": {
    "php": ">=8.3",
    "php-http/discovery": "^1.20",
    "psr/http-client-implementation": "*",
    "psr/http-client": "^1.0",
    "psr/http-factory-implementation": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3",
    "symfony/http-client": "^7.2",
    "nyholm/psr7": "^1.8",
    "friendsofphp/php-cs-fixer": "^3.64",
    "phpstan/phpstan": "^1.12",
    "phpunit/php-code-coverage": "^11.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}